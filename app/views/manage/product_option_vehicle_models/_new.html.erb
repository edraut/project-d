<% product_option_vehicle_model = new %>
Add a new product_option_vehicle_model</br>
<% form_for :product_option_vehicle_model, product_option_vehicle_model, :url => manage_product_option_vehicle_models_url,
	:html => {
		:ajax_binding => 'ajax_form',
		:ajax_success_element => 'product_option_vehicle_model_list',
		:ajax_success_placement => 'append',
		:ajax_success_callback => "$('#product_option_vehicle_model_new_link').click();",
		:ajax_error_element => 'product_option_vehicle_model_new',
		:ajax_method => 'POST'
	} do |f| %>
	<div class="span-15 last">
				<div class="span-3">
			<%= product_option_vehicle_model.first_error_on :vehicle_model_id %>
			<%= f.text_field :vehicle_model_id, :class => 'textfield-3' %>
		</div>
				<div class="span-3">
			<%= product_option_vehicle_model.first_error_on :product_option_id %>
			<%= f.text_field :product_option_id, :class => 'textfield-3' %>
		</div>
				<div class="span-3">
			<%= product_option_vehicle_model.first_error_on :year_begin %>
			<%= f.text_field :year_begin, :class => 'textfield-3' %>
		</div>
				<div class="span-3">
			<%= product_option_vehicle_model.first_error_on :year_end %>
			<%= f.text_field :year_end, :class => 'textfield-3' %>
		</div>
		
		<div class="span-3 last">
<%= f.submit :add %>
		</div>
	</div>
<% end %>
<%= link_to '', new_manage_product_option_vehicle_model_url,
 	:ajax_method => 'GET',
	:ajax_binding => 'ajax_link',
	:ajax_success_element => 'product_option_vehicle_model_new',
	:id => 'product_option_vehicle_model_new_link'
	%>