<div id="product_option_vehicle_model_list_<%= @owner.id %>" class="span-15 last">
	<div id="product_option_vehicle_model_list_header_<%= @owner.id %>" class="span-15 last">
		<div class="span-3">
		Vehicle model
	</div>
		<div class="span-1_5 column">
		Year begin
	</div>
		<div class="span-1_5 column">
		Year end
	</div>
	
	<div class="span-2 last">
		&nbsp;
	</div>
	</div>
	<% for product_option_vehicle_model in @product_option_vehicle_models %>
		<%= render :partial => "manage/product_option_vehicle_models/element_container_checkbox", :object => product_option_vehicle_model %>
	<% end %>
</div>
<div id="product_option_vehicle_model_new" class="span-8 last">
	<% for vehicle_model in @unused_vehicle_models %>
		<%= render :partial => "manage/product_option_vehicle_models/new_checkbox_container", :object => ProductOptionVehicleModel.new(:product_option_id => @owner.id, :vehicle_model_id => vehicle_model.id) %>
	<% end %>
</div>
