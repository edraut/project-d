<div class="span-24 last dash-foreground-color container-padding">
<h1>coupons</h1>
<div id="coupon_list" class="span-18 last">
	<div id="coupon_list_header" class="span-18 last">
		<div class="span-3">
		Coupon code
	</div>
		<div class="span-3">
		Flat amount
	</div>
		<div class="span-3">
		Percent off
	</div>
		<div class="span-3">
		Single use
	</div>
		<div class="span-3">
		Active
	</div>
	
	<div class="span-3 last">
		&nbsp;
	</div>
	</div>
	<% for coupon in @coupons %>
		<%= render :partial => "manage/coupons/element_container", :object => coupon %>
	<% end %>
</div>
<div id="coupon_new" class="span-18 last border-top margin-top">
	<%= render :partial => "manage/coupons/new", :object => Coupon.new %>
</div>
</div>